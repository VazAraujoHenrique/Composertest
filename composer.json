{
    "name": "cmelight/site",
    "description": "test",
    "config": {
    "preferred-install": "dist",
    "cache-files-ttl":0
  },
    "repositories":[
        {
            "type":"composer",
            "url":"https://wpackagist.org"
        },
        {
      "type": "package",
      "package": {
        "name": "uncanny-automator",
        "version": "2.5",
        "type": "wordpress-plugin",
        "dist": {
          "type": "zip",
          "url": "https://github.com/VazAraujoHenrique/Composertest/uncanny-automator.zip"
        }
      }
      }
    ],
    "require": {
        "wpackagist-plugin/popup-maker":"dev-trunk",
        "wpackagist-plugin/uncanny-learndash-toolkit":"3.*",
        "wordpress-plugin/uncanny-automator":"2.5"
    },
    "autoload": {
        "psr-0": {
            "cmelight": "src/"
        }
    },
      "extra": {
        "installer-paths": {
            "../public_html/wp-content/plugins/{$name}": ["wpackagist-plugin/popup-maker","wpackagist-plugin/uncanny-learndash-toolkit","wordpress-plugin/uncanny-automator"]
        }
      }
}